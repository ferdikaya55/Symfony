{% extends 'frontbase.html.twig' %}

{% block title %}Üye Bilgileri{% endblock %}
{% block sidebar %} {% include ('home/_usersidebar.html.twig') %} {% endblock %}
{% block body %}
<div class="span9">
    <ul class="breadcrumb">
        <li><a href="{{ path('home') }}">Anasayfa</a> <span class="divider">/</span></li>
        <li><a href="#">Üye Bilgileri</a> <span class="divider">/</span></li>
        <li class="active">Üye</li>
    </ul>
    <div class="row">

        <div class="span3" style="padding-left: 10px">
            <h5>Üye Profil Bilgileri </h5>
            <table class="table">
                <tbody>
                <tr>
                    <th>Id</th>
                    <td>{{ app.user.id }}</td>
                </tr>
                <tr>
                    <th>Email</th>
                    <td>{{ app.user.email }}</td>
                </tr>
                <tr>
                    <th>Roles</th>
                    <td>{{ app.user.roles|join }}</td>
                </tr>
                <tr>
                    <th>Name</th>
                    <td>{{ app.user.name }}</td>
                </tr>
                <tr>
                    <th>Address</th>
                    <td>{{ app.user.address }}</td>
                </tr>
                <tr>
                    <th>City</th>
                    <td>{{ app.user.city }}</td>
                </tr>
                <tr>
                    <th>Phone</th>
                    <td>{{ app.user.phone }}</td>
                </tr>
                <tr>
                    <th>Status</th>
                    <td>{{ app.user.status }}</td>
                </tr>
                </tbody>
            </table>
        </div>
    </div>

</div>
    {% endblock %}

